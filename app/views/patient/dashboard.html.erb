<div id="overlay">
  <div>
    <p>Are you sure you want to delete?</p>
    <p id="buttons">
      <button class="btn" onclick="cancelDelete();">Cancel</button>
      <button class="btn" onclick="deleteEncounter();">Yes</button>&nbsp;
    </p>
  </div>
</div>


<style>

#overlay button {
  height: 90px;
  width: 150px;
  font-weight: bold;
  font-size: 25px;
}

#overlay #buttons {
  float: right;
  top: 20%;
  position: relative;
}

#overlay p {
  font-size: 18px;
  font-weight: bold;
}

#overlay {
     visibility: hidden;
     position: absolute;
     left: 0px;
     top: 0px;
     width:100%;
     height:100%;
     text-align:center;
     z-index: 1000;
}


#overlay div {
     width:600px;
     margin: 100px auto;
     background-color: tomato; /*#fff;*/
     border:1px solid #000;
     padding:15px;
     text-align:center;
     height:200px;
}

#overlay {
     visibility: hidden;
     position: absolute;
     left: 0px;
     top: 0px;
     width:100%;
     height:100%;
     text-align:center;
     z-index: 1000;
     background-image:url(background-trans.png);
}





</style>


<script>

var selectedEncounter = null;
var tabName = null;

function voidEncounter(e, tab_name) {
  el = document.getElementById("overlay");
  selectedEncounter = e.id;
  tabName = tab_name;
  el.style.visibility = (el.style.visibility == "visible") ? "hidden" : "visible";
}

function cancelDelete() {
  el = document.getElementById("overlay");
  el.style.visibility = 'hidden';
}

function deleteEncounter() {
  document.location = "/void_encounter/" + selectedEncounter + "/" + tabName;
}

</script>


